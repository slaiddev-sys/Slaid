{
  "crons": [
    {
      "path": "/api/cron/renew-credits",
      "schedule": "0 0 1 * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path((?!api/webhooks).*)",
      "has": [
        {
          "type": "host",
          "value": "slaidapp.com"
        }
      ],
      "destination": "https://www.slaidapp.com/:path*",
      "permanent": true
    }
  ]
}
